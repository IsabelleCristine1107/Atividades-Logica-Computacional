programa {
  funcao inicio() {
    //Declaração de Variáveis
    cadeia nome
    inteiro idade

    //Atribuição direta de Variáveis
    nome = "Isabelle"
    idade = 16

    escreva ("Nome: ", nome)
    escreva("\nIdade:", idade)

    //Atribuições comando Leia()
    escreva("\nDigite o seu nome: ")
    leia(nome)
    escreva("Digite o seu idade: ")
    leia(idade)

    escreva("nome: ", nome)
    escreva("\nidade: ", idade)

  }
}
